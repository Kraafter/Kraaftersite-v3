datasource db {
  provider = "sqlite"
  url      = "file:./gal.db"
}

generator client {
  provider = "prisma-client-js"
}

model CachedArt {
  id        Int      @id @default(autoincrement())
  data      String
  createdAt DateTime @default(now())
}

model CachedArtchive {
  id        Int      @id @default(autoincrement())
  data      String
  createdAt DateTime @default(now())
}

model CachedPhoto {
  id        Int      @id @default(autoincrement())
  data      String
  createdAt DateTime @default(now())
}